                this.log(`<span class="text-red">Commande non reconnue.</span>`);\par
            \}\par
        \}\par
    \}\par
\par
    const randomChoice = (arr) => arr[Math.floor(Math.random() * arr.length)];\par
    const shuffleString = (str) => str.split('').sort(() => 0.5 - Math.random()).join('');\par
\par
    let core;\par
