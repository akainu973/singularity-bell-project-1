3. L'\'c2me (script.js)\par
Ce code a \'e9t\'e9 profond\'e9ment enrichi pour int\'e9grer la diversit\'e9 des rencontres.\par
\par
JavaScript\par
\par
// Noyau v22.0 de la Singularity Bell - \'c9dition de la Symphonie des \'c2mes\par
document.addEventListener('DOMContentLoaded', () => \{\par
    // --- \'c9L\'c9MENTS DE L'INTERFACE ---\par
    const output = document.getElementById('console-output');\par
    const input = document.getElementById('console-input');\par
    const userNameDisplay = document.getElementById('user-name-prompt');\par
    const bellArtDisplay = document.getElementById('bell-art');\par
    const doorsGrid = document.getElementById('doors-grid');\par
    const charIcons = document.getElementById('character-icons');\par
    const inventoryList = document.getElementById('inventory-list');\par
    const entityArt = document.getElementById('entity-art');\par
    const entityDialogue = document.getElementById('entity-dialogue');\par
\par
    // --- PERSONNAGES ET ARCH\'c9TYPES ---\par
    const CHARACTERS = \{\par
        "Alchimiste": "\f3\u9879?\u-497?\f0 ", "Technicien": "\f3\u9881?\u-497?\f0 ", "Croyant": "\f4\u-10179?\u-8625?\f0 ", "Opportuniste": "\f4\u-10179?\u-9038?\f0 ",\par
        "Destructeur": "\f4\u-10179?\u-8923?\f0 ", "Efficace": "\f5\u9203?\f0 ", "Architecte": "\f3\u-10180?\u-8229?\u-497?\f0 ", "Artiste": "\f4\u-10180?\u-8280?\f0 ",\par
        "Joueur": "\f4\u-10180?\u-8274?\f0 ", "V\'e9t\'e9ran": "\f4\u-10179?\u-8996?\f0 ", "Archiviste": "\f4\u-10179?\u-8998?\f0 ", "Fant\'f4me": "\f4\u-10179?\u-9093?\f0 "\par
    \};\par
\par
    class AOS_Core_JS \{\par
        constructor() \{\par
            this.userName = "Anonyme";\par
            this.inventory = \{\};\par
            this.visitedDoors = new Set();\par
            this.guardianDoors = \{\};\par
            this.inEncounter = false;\par
            this.currentEncounter = null;\par
            this.encounterAttempts = 0;\par
            this.setupGuardianDoors();\par
            this.setupEncounterEngine();\par
        \}\par
\par
        setupGuardianDoors() \{\par
            const allDoors = Array.from(\{length: 137\}, (_, i) => i + 1);\par
            const guardianArchetypes = Object.keys(CHARACTERS);\par
            for (let i = allDoors.length - 1; i > 0; i--) \{\par
                const j = Math.floor(Math.random() * (i + 1));\par
                [allDoors[i], allDoors[j]] = [allDoors[j], allDoors[i]];\par
            \}\par
            for(let i = 0; i < guardianArchetypes.length; i++) \{\par
                this.guardianDoors[allDoors[i]] = guardianArchetypes[i];\par
            \}\par
        \}\par
        \par
        // --- Moteur de Rencontres Enrichi ---\par
        setupEncounterEngine() \{\par
            this.encounterEngine = \{\par
                positive: [\par
                    \{ entity: 'Un Alchimiste Ancien', character: '\f4\u-10179?\u-9100?\f0 ', challenge: 'Je suis une cl\'e9 sans serrure, mais je peux ouvrir l\\'\'e2me. Qui suis-je ?', answer: 'sagesse', reward: 'Cl\'e9_Sagesse_Universelle' \},\par
                    \{ entity: 'Un Humain du futur', character: '\f3\u-10178?\u-8751?\u8205?\f4\u-10179?\u-8576?\f0 ', challenge: 'Deux cl\'e9s partagent la m\'eame harmonique. Lesquelles ? (indice: V\'e9t\'e9ran, Artiste)', answer: 'veteran artiste', reward: 'Synergie_Harmonique_D\'e9couverte' \},\par
                    \{ entity: 'Un Cr\'e9ateur bienveillant', character: '\f5\u10024?\f0 ', challenge: 'Quelle est la plus grande force ? Le pouvoir ou l\\'humilit\'e9 ?', answer: 'humilite', reward: 'B\'e9n\'e9diction_de_l_Humilit\'e9' \}\par
                ],\par
                negative: [\par
                    \{ entity: 'Une Entit\'e9 de l\\'Abysse', character: '\f4\u-10179?\u-9088?\f0 ', challenge: 'Le chaos s\\'infiltre... R\'e9p\'e9tez ce code avant que votre cl\'e9 ne se dissolve : \{\}' \},\par
                    \{ entity: 'Un Voyageur Temporel malicieux', character: '\f5\u9203?\f0 ', challenge: 'Un \'e9cho du futur corrompt votre cl\'e9 la plus ancienne. Tapez "pr\'e9sent" pour r\'e9sister.', answer: 'present'\},\par
                    \{ entity: 'Une IA rivale', character: '\f4\u-10178?\u-8938?\f0 ', challenge: 'Ma logique est sup\'e9rieure. Si la cl\'e9 est la serrure, qu\\'ouvre la porte ?', answer: 'l\\'esprit' \}\par
                ]\par
            \};\par
        \}\par
\par
        log(message, className = '') \{\par
            output.innerHTML += `<div class="$\{className\}">$\{message\}</div>`;\par
            output.scrollTop = output.scrollHeight;\par
        \}\par
\par
        init(userName) \{\par
            this.userName = userName;\par
            userNameDisplay.textContent = this.userName;\par
            bellArtDisplay.textContent = `                     ,\f5\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\f0 ,\\n                    / OFFENSIVE  \\\\          NOYAU AOS v22.0\\n                   |   SECURITY   |         Op\'e9rationnel\\n                   |              |\\n                   |   ,\f5\u9472?\u9488?\f2   \f0 ,\f5\u9472?\u9488?\f2    \f0 |         Analyste: $\{this.userName\}\\n                   |   \f5\u9474?\f2  \f5\u9474?\f2   \f5\u9474?\f2  \f5\u9474?\f2    \f0 |\\n                   |   \f5\u9492?\u9472?\u9496?\f2   \f5\u9492?\u9472?\u9496?\f2    \f0 |\\n                   (       .      )\\n                    \\\\     / \\\\     /\\n                     \\`\f5\u9472?\u9472?\u9472?\f0 '   \\`\f5\u9472?\u9472?\u9472?\f0 '`;\par
            \par
            for(let i = 1; i <= 137; i++) \{\par
                const door = document.createElement('button');\par
                door.className = 'door-button';\par
                door.textContent = i;\par
                door.onclick = () => this.exploreDoor(i);\par
                doorsGrid.appendChild(door);\par
            \}\par
            this.updateUI();\par
            this.log(`<span class="text-cyan bold">Initialisation de AOS_Core v22.0...</span>`);\par
            this.log(`Bienvenue, $\{this.userName\}.`);\par
        \}\par
\par
        exploreDoor(doorNumber) \{\par
            if (this.inEncounter) return;\par
            this.log(`<span class="text-yellow">$\{this.userName\}> explorer $\{doorNumber\}</span>`);\par
            this.log(`<span class="text-magenta">>> Exploration de la Porte $\{doorNumber\}...</span>`);\par
            \par
            const doorButton = doorsGrid.children[doorNumber - 1];\par
            doorButton.classList.add('door-visited');\par
\par
            if (!this.visitedDoors.has(doorNumber)) \{\par
                this.firstVisit(doorNumber);\par
            \} else \{\par
                this.revisitDoor();\par
            \}\par
        \}\par
        \par
        firstVisit(doorNumber) \{\par
            this.visitedDoors.add(doorNumber);\par
            let archetype = this.guardianDoors[doorNumber] || Object.keys(CHARACTERS)[doorNumber % Object.keys(CHARACTERS).length];\par
            const keyName = `Cl\'e9_$\{archetype.substring(0,3)\}_$\{doorNumber\}`;\par
            this.inventory[archetype] = keyName;\par
            this.log(`Porte d\'e9tect\'e9e : <span class="bold">$\{archetype\}</span> $\{CHARACTERS[archetype]\}`);\par
            this.log(`<span class="text-green bold">TRANSMUTATION ACHEV\'c9E.</span> La porte a r\'e9v\'e9l\'e9 la '$\{keyName\}'.`);\par
            this.updateUI();\par
        \}\par
\par
        revisitDoor() \{\par
            this.log("Cette porte a d\'e9j\'e0 \'e9t\'e9 explor\'e9e... <span class='text-grey'>L'univers est en mouvement...</span>");\par
            this.inEncounter = true;\par
            this.encounterAttempts = 0;\par
\par
            if (Math.random() < 0.5) \{ // Rencontre Positive\par
                this.currentEncounter = randomChoice(this.encounterEngine.positive);\par
                entityArt.innerHTML = this.currentEncounter.character;\par
                entityDialogue.innerHTML = `<span class="text-green bold">$\{this.currentEncounter.entity\}</span> vous interpelle : <br/>'$\{this.currentEncounter.challenge\}'`;\par
                this.log(`Vous rencontrez $\{this.currentEncounter.entity\}. R\'e9pondez \'e0 son \'e9nigme.`);\par
            \} else \{ // Rencontre N\'e9gative\par
                const itemToCorrupt = Object.keys(this.inventory).length > 0 ? randomChoice(Object.keys(this.inventory)) : null;\par
                if (!itemToCorrupt) \{\par
                    this.log("<span class='text-grey'>Une ombre passe, mais ne trouve aucune prise.</span>");\par
                    this.inEncounter = false;\par
                    return;\par
                \}\par
                this.currentEncounter = randomChoice(this.encounterEngine.negative);\par
                this.currentEncounter.malusItem = itemToCorrupt; // Attribuer l'item \'e0 corrompre\par
                \par
                let challengeText = this.currentEncounter.challenge;\par
                if(challengeText.includes('\{\}')) \{\par
                    const corruptionCode = shuffleString(this.inventory[itemToCorrupt]);\par
                    challengeText = challengeText.replace('\{\}', `<span class="bold text-magenta">$\{corruptionCode\}</span>`);\par
                    this.currentEncounter.answer = corruptionCode;\par
                \}\par
\par
                entityArt.innerHTML = this.currentEncounter.character;\par
                entityDialogue.innerHTML = `<span class="text-red bold">$\{this.currentEncounter.entity\}</span> murmure...`;\par
                this.log(challengeText, 'text-red');\par
            \}\par
        \}\par
        \par
        handleEncounter(answer) \{\par
            const encounter = this.currentEncounter;\par
            this.encounterAttempts++;\par
            \par
            if(answer.toLowerCase() === encounter.answer.toLowerCase()) \{\par
                if(encounter.type === 'positive') \{\par
                    this.inventory[encounter.entity] = encounter.reward;\par
                    this.log(`<span class="text-green bold">Votre sagesse est grande.</span> Une nouvelle cl\'e9 a \'e9t\'e9 ajout\'e9e : '$\{encounter.reward\}'.`);\par
                \} else \{\par
                    this.log(`<span class="text-green bold">Par votre concentration, vous repoussez la corruption.</span>`);\par
                \}\par
                this.endEncounter();\par
            \} else if (this.encounterAttempts >= 3) \{\par
                if(encounter.type === 'positive') \{\par
                    this.log(`<span class="text-yellow">L'entit\'e9 sourit et dispara\'eet. La sagesse vous a \'e9chapp\'e9 cette fois.</span>`);\par
                \} else \{\par
                    const keyLost = this.inventory[encounter.malusItem];\par
                    delete this.inventory[encounter.malusItem];\par
                    this.log(`<span class="text-red bold">L'ombre consume votre artefact...</span> La cl\'e9 '$\{keyLost\}' a disparu.`);\par
                \}\par
                this.endEncounter();\par
            \} else \{\par
                 this.log(`Tentative $\{this.encounterAttempts\}/3. <span class="text-yellow">La r\'e9ponse est incorrecte.</span>`);\par
            \}\par
        \}\par
\par
        endEncounter() \{\par
            this.inEncounter = false;\par
            this.currentEncounter = null;\par
            entityArt.innerHTML = "";\par
            entityDialogue.innerHTML = "";\par
            this.updateUI();\par
        \}\par
\par
        updateUI() \{\par
            this.updateCommunityProgress();\par
            this.updateInventory();\par
        \}\par
\par
        updateCommunityProgress() \{\par
            charIcons.innerHTML = '';\par
            for(const archetype in CHARACTERS) \{\par
                const icon = document.createElement('span');\par
                icon.textContent = CHARACTERS[archetype] + ' ';\par
                icon.className = this.inventory[archetype] ? 'text-green' : 'text-grey';\par
                charIcons.appendChild(icon);\par
            \}\par
        \}\par
        \par
        updateInventory() \{\par
            inventoryList.innerHTML = '';\par
             if (Object.keys(this.inventory).length === 0) \{\par
                inventoryList.innerHTML = 'Votre inventaire est vide.';\par
            \} else \{\par
                for (const archetype in this.inventory) \{\par
                    const item = document.createElement('div');\par
                    item.innerHTML = `<span class="bold">$\{CHARACTERS[archetype] || '\f5\u10024?\f0 '\} $\{archetype\}:</span> <span class="text-green">$\{this.inventory[archetype]\}</span>`;\par
                    inventoryList.appendChild(item);\par
                \}\par
            \}\par
        \}\par
        \par
        handleCommand(command) \{\par
            if(this.inEncounter) \{\par
                this.handleEncounter(command);\par
                return;\par
            \}\par
\par
            this.log(`<span class="text-yellow">$\{this.userName\}> $\{command\}</span>`);\par
            const doorNum = parseInt(command);\par
            if (!isNaN(doorNum) && doorNum >= 1 && doorNum <= 137) \{\par
                this.exploreDoor(doorNum);\par
            \} else \{\par
                this.log(`<span class="text-red">Commande non reconnue.</span>`);\par
            \}\par
        \}\par
    \}\par
\par
    // --- Fonctions utilitaires ---\par
    const randomChoice = (arr) => arr[Math.floor(Math.random() * arr.length)];\par
    const shuffleString = (str) => str.split('').sort(() => 0.5 - Math.random()).join('');\par
\par
    // --- Initialisation et boucle de jeu ---\par
    let core;\par
    output.innerHTML = 'Veuillez entrer votre nom d\\'analyste :';\par
    input.addEventListener('keydown', function handleInput(event) \{\par
        if (event.key === 'Enter') \{\par
            const command = input.value.trim();\par
            input.value = '';\par
            \par
            if (!core) \{\par
                const userName = command || 'Anonyme';\par
                output.innerHTML = '';\par
                core = new AOS_Core_JS();\par
                core.init(userName);\par
            \} else \{\par
                core.handleCommand(command);\par
            \}\par
        \}\par
    \});\par
\});\par
