             bellArtDisplay.className = colorClass;\par
             userNameDisplay.className = `bold $\{colorClass\}`;\par
             bellArtDisplay.textContent = `                     ,\f5\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\f0 ,\\n                    / OFFENSIVE  \\\\          NOYAU AOS v23.0\\n                   |   SECURITY   |         Op\'e9rationnel\\n                   |              |\\n                   |   ,\f5\u9472?\u9488?\f2   \f0 ,\f5\u9472?\u9488?\f2    \f0 |         Analyste: $\{this.userName\}\\n                   |   \f5\u9474?\f2  \f5\u9474?\f2   \f5\u9474?\f2  \f5\u9474?\f2    \f0 |\\n                   |   \f5\u9492?\u9472?\u9496?\f2   \f5\u9492?\u9472?\u9496?\f2    \f0 |\\n                   (       .      )\\n                    \\\\     / \\\\     /\\n                     \\`\f5\u9472?\u9472?\u9472?\f0 '   \\`\f5\u9472?\u9472?\u9472?\f0 '`;\par
        \}\par
        \par
        updateCommunityProgress() \{\par
            charIcons.innerHTML = '';\par
            for(const archetype in CHARACTERS) \{\par
                const icon = document.createElement('span');\par
