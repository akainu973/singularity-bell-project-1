            const allDoors = Array.from(\{length: 137\}, (_, i) => i + 1);\par
            const guardianArchetypes = Object.keys(CHARACTERS);\par
            for (let i = allDoors.length - 1; i > 0; i--) \{\par
                const j = Math.floor(Math.random() * (i + 1));\par
                [allDoors[i], allDoors[j]] = [allDoors[j], allDoors[i]];\par
            \}\par
            for(let i = 0; i < guardianArchetypes.length; i++) \{\par
                this.guardianDoors[allDoors[i]] = guardianArchetypes[i];\par
            \}\par
        \}\par
        \par
        setupEncounterEngine() \{\par
            this.encounterEngine = \{\par
                positive: [\par
                    \{ entity: 'Un Alchimiste Ancien', character: '\f4\u-10179?\u-9100?\f0 ', challenge: 'Je suis une cl\'e9 sans serrure, mais je peux ouvrir l\\'\'e2me. Qui suis-je ?', answer: 'sagesse', reward: 'Cl\'e9_Sagesse_Universelle' \},\par
                    \{ entity: 'Un Humain du futur', character: '\f3\u-10178?\u-8751?\u8205?\f4\u-10179?\u-8576?\f0 ', challenge: 'Deux cl\'e9s partagent la m\'eame harmonique. Lesquelles ? (indice: V\'e9t\'e9ran, Artiste)', answer: 'veteran artiste', reward: 'Synergie_Harmonique_D\'e9couverte' \},\par
                ],\par
                negative: [\par
                    \{ entity: 'Une Entit\'e9 de l\\'Abysse', character: '\f4\u-10179?\u-9088?\f0 ', challenge: 'Le chaos s\\'infiltre... R\'e9p\'e9tez ce code avant que votre cl\'e9 ne se dissolve : \{\}' \},\par
                    \{ entity: 'Une IA rivale', character: '\f4\u-10178?\u-8938?\f0 ', challenge: 'Ma logique est sup\'e9rieure. Si la cl\'e9 est la serrure, qu\\'ouvre la porte ?', answer: 'l\\'esprit' \}\par
                ]\par
            \};\par
        \}\par
\par
        log(message, className = '', useTyping = true) \{\par
            const line = document.createElement('div');\par
            line.className = className;\par
            output.appendChild(line);\par
            if (useTyping) \{\par
                typeWriter(line, message);\par
            \} else \{\par
                line.innerHTML = message;\par
            \}\par
            output.scrollTop = output.scrollHeight;\par
        \}\par
        \par
        init(userName) \{\par
            this.userName = userName;\par
            this.updateUI();\par
            const welcomeMessage1 = `<span class="text-cyan bold">Initialisation de AOS_Core v25.0...</span>`;\par
            const welcomeMessage2 = `Bienvenue, $\{this.userName\}.`;\par
            this.log(welcomeMessage1, '', true);\par
            setTimeout(() => this.log(welcomeMessage2, '', true), 800);\par
        \}\par
\par
        exploreDoor(doorNumber) \{\par
            if (this.inEncounter) return;\par
            this.log(`<span class="text-yellow">$\{this.userName\}> explorer $\{doorNumber\}</span>`, '', false);\par
            this.log(`<span class="text-magenta">>> Exploration de la Porte $\{doorNumber\}...</span>`);\par
            \par
            const doorButton = doorsGrid.children[doorNumber - 1];\par
            doorButton.classList.add('door-visited');\par
\par
            if (!this.visitedDoors.has(doorNumber)) \{\par
                 // --- LOGIQUE CORRIG\'c9E ---\par
                if (Math.random() < 0.5) \{\par
                    this.log("La porte r\'e9v\'e8le son secret sans r\'e9sistance...");\par
                    this.firstVisit(doorNumber);\par
                \} else \{\par
                    this.log("Le chemin vers la connaissance est une \'e9preuve...");\par
                    this.revisitDoor(true, doorNumber); // isFirstVisit = true\par
                \}\par
            \} else \{\par
                this.revisitDoor(false, null); // isFirstVisit = false\par
            \}\par
        \}\par
        \par
        firstVisit(doorNumber) \{\par
            this.visitedDoors.add(doorNumber);\par
            let archetype = this.guardianDoors[doorNumber] || Object.keys(CHARACTERS)[doorNumber % Object.keys(CHARACTERS).length];\par
            const chaosArchetypes = ["Destructeur", "Joueur", "Opportuniste"];\par
            if (chaosArchetypes.includes(archetype)) this.chaosLevel++; else this.chaosLevel--;\par
            const keyName = `Cl\'e9_$\{archetype.substring(0,3)\}_$\{doorNumber\}`;\par
            this.inventory[archetype] = keyName;\par
            this.log(`Porte d\'e9tect\'e9e : <span class="bold">$\{archetype\}</span> $\{CHARACTERS[archetype]\}`);\par
            this.log(`<span class="text-green bold">TRANSMUTATION ACHEV\'c9E.</span> La porte a r\'e9v\'e9l\'e9 la '$\{keyName\}'.`);\par
            this.updateUI();\par
        \}\par
\par
        revisitDoor(isFirstVisit = false, doorNumber = null) \{\par
            this.log(isFirstVisit ? "" : "Cette porte a d\'e9j\'e0 \'e9t\'e9 explor\'e9e... <span class='text-grey'>L'univers est en mouvement...</span>");\par
            this.inEncounter = true;\par
            this.encounterAttempts = 0;\par
\par
            const encounterPool = Math.random() < 0.5 * (1 - this.chaosLevel * 0.1) ? this.encounterEngine.positive : this.encounterEngine.negative;\par
            this.currentEncounter = \{...randomChoice(encounterPool), isFirstVisit, doorNumber\};\par
            \par
            if (this.currentEncounter.type === 'negative' && Object.keys(this.inventory).length === 0 && !isFirstVisit) \{\par
                 this.log("<span class='text-grey'>Une ombre passe, mais ne trouve aucune prise.</span>");\par
                 this.endEncounter(); return;\par
            \}\par
\par
            entityArt.innerHTML = this.currentEncounter.character;\par
            const entityColor = this.currentEncounter.type === 'positive' ? 'text-green' : 'text-red';\par
            entityDialogue.innerHTML = `<span class="$\{entityColor\} bold">$\{this.currentEncounter.entity\}</span> vous interpelle...`;\par
            \par
            let challengeText = this.currentEncounter.challenge;\par
            if(challengeText.includes('\{\}')) \{\par
                const itemToCorrupt = randomChoice(Object.keys(this.inventory));\par
                this.currentEncounter.malusItem = itemToCorrupt;\par
                const corruptionCode = shuffleString(this.inventory[itemToCorrupt]);\par
                challengeText = challengeText.replace('\{\}', `<span class="bold text-magenta">$\{corruptionCode\}</span>`);\par
                this.currentEncounter.answer = corruptionCode;\par
            \}\par
\par
            setTimeout(() => \{\par
                entityDialogue.innerHTML = `<span class="$\{entityColor\} bold">$\{this.currentEncounter.entity\}</span>: <br/>'$\{challengeText\}'`;\par
            \}, 1000);\par
        \}\par
        \par
        handleEncounter(answer) \{\par
            const encounter = this.currentEncounter;\par
            this.encounterAttempts++;\par
            \par
            if(answer.toLowerCase() === encounter.answer?.toLowerCase()) \{\par
                if(encounter.isFirstVisit) \{ this.firstVisit(encounter.doorNumber); \}\par
                else if (encounter.type === 'positive') \{\par
                    this.inventory[encounter.entity] = encounter.reward;\par
                    this.log(`<span class="text-green bold">Votre sagesse est grande.</span> Cl\'e9 ajout\'e9e : '$\{encounter.reward\}'.`);\par
                \} else \{ this.log(`<span class="text-green bold">Vous repoussez la corruption.</span>`); \}\par
                this.endEncounter();\par
            \} else if (this.encounterAttempts >= 3) \{\par
                if(encounter.type === 'positive') \{\par
                    this.log(`<span class="text-yellow">L'entit\'e9 dispara\'eet. La sagesse vous a \'e9chapp\'e9.</span>`);\par
                \} else \{\par
                    const keyLost = this.inventory[encounter.malusItem];\par
                    delete this.inventory[encounter.malusItem];\par
                    this.log(`<span class="text-red bold">L'ombre consume votre artefact...</span> La cl\'e9 '$\{keyLost\}' a disparu.`);\par
                \}\par
                if (encounter.isFirstVisit) \{ this.log("La porte reste scell\'e9e pour l'instant."); \}\par
                this.endEncounter();\par
            \} else \{\par
                 this.log(`Tentative $\{this.encounterAttempts\}/3. <span class="text-yellow">Incorrect.</span>`);\par
            \}\par
        \}\par
\par
        endEncounter() \{\par
            this.inEncounter = false;\par
            this.currentEncounter = null;\par
            setTimeout(() => \{\par
                entityArt.innerHTML = "";\par
                entityDialogue.innerHTML = "";\par
            \}, 2000);\par
            this.updateUI();\par
        \}\par
\par
        updateUI() \{\par
            this.updateBell();\par
            this.updateCommunityProgress();\par
            this.updateInventory();\par
        \}\par
\par
        updateBell() \{\par
             let colorClass = "text-yellow";\par
             if (this.chaosLevel > 2) colorClass = "text-magenta";\par
             if (this.chaosLevel < -2) colorClass = "text-cyan";\par
             bellArtDisplay.className = colorClass;\par
             userNameDisplay.className = `bold $\{colorClass\}`;\par
             bellArtDisplay.textContent = `                     ,\f5\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\u9472?\f0 ,\\n                    / OFFENSIVE  \\\\          NOYAU AOS v25.0\\n                   |   SECURITY   |         Op\'e9rationnel\\n                   |              |\\n                   |   ,\f5\u9472?\u9488?\f2   \f0 ,\f5\u9472?\u9488?\f2    \f0 |         Analyste: $\{this.userName\}\\n                   |   \f5\u9474?\f2  \f5\u9474?\f2   \f5\u9474?\f2  \f5\u9474?\f2    \f0 |\\n                   |   \f5\u9492?\u9472?\u9496?\f2   \f5\u9492?\u9472?\u9496?\f2    \f0 |\\n                   (       .      )\\n                    \\\\     / \\\\     /\\n                     \\`\f5\u9472?\u9472?\u9472?\f0 '   \\`\f5\u9472?\u9472?\u9472?\f0 '`;\par
        \}\par
        \par
        updateCommunityProgress() \{\par
            charIcons.innerHTML = '';\par
            for(const archetype in CHARACTERS) \{\par
                const icon = document.createElement('span');\par
                icon.textContent = CHARACTERS[archetype] + ' ';\par
                icon.className = this.inventory[archetype] ? 'text-green' : 'text-grey';\par
                charIcons.appendChild(icon);\par
            \}\par
        \}\par
        \par
        updateInventory() \{\par
            inventoryList.innerHTML = '';\par
             if (Object.keys(this.inventory).length === 0) \{\par
                inventoryList.innerHTML = 'Votre inventaire est vide.';\par
            \} else \{\par
                for (const archetype in this.inventory) \{\par
                    const item = document.createElement('div');\par
                    item.innerHTML = `<span class="bold">$\{CHARACTERS[archetype] || '\f5\u10024?\f0 '\} $\{archetype\}:</span> <span class="text-green">$\{this.inventory[archetype]\}</span>`;\par
                    inventoryList.appendChild(item);\par
                \}\par
            \}\par
        \}\par
        \par
        handleCommand(command) \{\par
            if(this.inEncounter) \{\par
                this.log(`<span class="text-yellow">$\{this.userName\}> $\{command\}</span>`, '', false);\par
                this.handleEncounter(command);\par
                return;\par
            \}\par
            \par
            const commandParts = command.split(' ');\par
            const action = commandParts[0].toLowerCase();\par
\par
            if (action === "explorer" && commandParts.length === 2) \{\par
                const doorNum = parseInt(commandParts[1]);\par
                 if (!isNaN(doorNum) && doorNum >= 1 && doorNum <= 137) \{\par
                    this.exploreDoor(doorNum);\par
                \} else \{\par
                    this.log(`<span class="text-red">Num\'e9ro de porte invalide.</span>`);\par
                \}\par
            \} else \{\par
                this.log(`<span class="text-yellow">$\{this.userName\}> $\{command\}</span>`, '', false);\par
                this.log(`<span class="text-red">Commande non reconnue.</span>`);\par
            \}\par
        \}\par
    \}\par
\par
    const randomChoice = (arr) => arr[Math.floor(Math.random() * arr.length)];\par
    const shuffleString = (str) => str.split('').sort(() => 0.5 - Math.random()).join('');\par
\par
    let core;\par
    const line1 = document.createElement('div');\par
    output.appendChild(line1);\par
    typeWriter(line1, 'Veuillez entrer votre nom d\\'analyste :', () => input.focus());\par
    input.addEventListener('keydown', function handleInput(event) \{\par
        if (event.key === 'Enter') \{\par
            const command = input.value.trim();\par
            input.value = '';\par
            \par
            if (!core) \{\par
                const userName = command || 'Anonyme';\par
                output.innerHTML = '';\par
                core = new AOS_Core_JS();\par
                core.init(userName);\par
            \} else \{\par
                core.handleCommand(command);\par
            \}\par
        \}\par
    \});\par
\});\par
}
